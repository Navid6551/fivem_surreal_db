{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["@citizenfx/client", "@citizenfx/server", "@types/node"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
